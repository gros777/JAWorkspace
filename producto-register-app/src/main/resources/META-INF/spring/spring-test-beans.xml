<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="dateFormat" class="java.text.SimpleDateFormat">
		<constructor-arg value="yyyy-MM-dd" />
	</bean>
	<bean name="registeredProductAction"
		class="me.victorhernandez.struts2.pra.actions.RegisteredProductsAction">
		<property name="products">
			<list>
				<ref bean="product1" />
				<ref bean="product2" />
				<ref bean="product3" />
			</list>
		</property>
	</bean>
	<bean name="product1" class="me.victorhernandez.struts2.pra.domanin.Product">
		<property name="productName" value="Macarrones"/>
		<property name="goodThough">
			<bean factory-bean="dateFormat" factory-method="parse">
				<constructor-arg value="2014-10-04" />
			</bean>
		</property>
		<property name="availables" value="25"/>
		<property name="mark" value="McKormic" />
		<property name="shelve" value="AC2345" />
		<property name="id" value="1" />
		<property name="version" value="2" />
	</bean>
	
	<bean name="product2" class="me.victorhernandez.struts2.pra.domanin.Product">
		<property name="productName" value="Super Jugo"/>
		<property name="goodThough">
			<bean factory-bean="dateFormat" factory-method="parse">
				<constructor-arg value="2014-10-04" />
			</bean>
		</property>
		<property name="availables" value="35"/>
		<property name="mark" value="Salud" />
		<property name="shelve" value="HR3498" />
		<property name="id" value="2" />
		<property name="version" value="3" />
	</bean>
	<bean name="product3" class="me.victorhernandez.struts2.pra.domanin.Product">
		<property name="productName" value="Sal de ajo"/>
		<property name="goodThough">
			<bean factory-bean="dateFormat" factory-method="parse">
				<constructor-arg value="2013-12-05" />
			</bean>
		</property>
		<property name="availables" value="78"/>
		<property name="mark" value="McKormic" />
		<property name="shelve" value="TD5196" />
		<property name="id" value="3" />
		<property name="version" value="7" />
	</bean>
	
	
</beans>